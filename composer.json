{
    "name": "selrahcd/rector-rules",
    "description": "A set of home made rector rules",
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^1.7",
        "rector/rector": "^0.13.6",
        "symplify/rule-doc-generator": "^11.0"
    },
    "authors": [
        {
            "name": "SelrahcD",
            "email": "c.desneuf@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "SelrahcD\\RectorRules\\" : "rules"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SelrahcD\\RectorRules\\Tests\\" : "rules-tests"
        }
    },
    "scripts": {
        "tests": "./vendor/bin/phpunit"
    }
}
